{
    "name": "lucid-hdl",
    "displayName": "vscode-lucid-hdl",
    "description": "Lucid Hardware Description language from embeddedmicro",
    "version": "0.0.1",
    "publisher": "dexterhaslem",
    "homepage": "https://github.com/DexterHaslem/vscode-lucid-hdl",
	"repository": {
		"type": "git",
		"url": "https://github.com/DexterHaslem/vscode-lucid-hdl.git"
	},
	"bugs": {
		"url": "https://github.com/DexterHaslem/vscode-lucid-hdl/issues"
	},
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lucidhdl",
            "aliases": ["Lucid HDL", "lucidhdl"],
            "extensions": [".luc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lucidhdl",
            "scopeName": "source.lucid",
            "path": "./syntaxes/lucidhdl.tmLanguage"
        }]
    }
}