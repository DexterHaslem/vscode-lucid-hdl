{
    "name": "lucid-hdl",
    "displayName": "Lucid HDL",
    "description": "Lucid Hardware Description Language",
    "version": "0.2.0",
    "publisher": "dexterhaslem",
    "homepage": "https://github.com/DexterHaslem/vscode-lucid-hdl",
    "repository": {
        "type": "git",
        "url": "https://github.com/DexterHaslem/vscode-lucid-hdl.git"
    },
    "bugs": {
        "url": "https://github.com/DexterHaslem/vscode-lucid-hdl/issues"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "lucidhdl",
                "aliases": [
                    "Lucid HDL",
                    "lucidhdl",
                    "lucid",
                    "mojo"
                ],
                "extensions": [
                    ".luc",
                    ".v",
                    ".vh"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "lucidhdl",
                "scopeName": "source.lucid",
                "path": "./syntaxes/lucidhdl.tmLanguage"
            }
        ]
    }
}